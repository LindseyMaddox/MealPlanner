<%= render 'shared/navbar' %>
<%= render 'shared/side_links.html.erb' %>
<div class="row">
          <div class="col-sm-3"></div>
          <div class="col-sm-6">
          <% @meal_plans.each do |meal| %>
            <li> <%= link_to meal.recipe.name, meal.recipe %> - <%= meal.meal_date %></li>
          <% end %>
          </div>
   <div class="col-sm-2">
          <div class="row">
              <div class="col-sm-12">
                <strong>Filter by Date</strong>
              </div>
              <div class="col-sm-12">
                <% @time_period.each_pair do |text, number| %>
                  <li>
                    <%= link_to "#{text}", meal_plans_path(date_filter: number), class: 'label label-primary' %>
                  </li>
                <% end %>
              </div>
          </div>
      </div>
</div>